    "jspsych": "^8.2.1",
    "multer": "^1.4.5-lts.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "wbat": "^0.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.2",
    "nodemon": "^3.1.9",
    "rollup-plugin-postcss": "^4.0.2"
  }
